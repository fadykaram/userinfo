	"description": "Displays a popup with information about users.",
	"homepage": "https://tas2580.net/downloads/phpbb-ajax-userinfo/",
	"version": "1.0.0-RC4",
	"keywords": ["phpbb", "extension", "user","ajax"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "tas2580",
			"homepage": "https://tas2580.net",
			"email": "mail@tas2580.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "AJAX Userinfo",
